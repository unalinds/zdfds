        private void cpf_CheckedChanged(object sender, EventArgs e)
        {
            cpf.Text = "";
            cpf.Mask = "000-000-000-00";
            cpf.MaskInputRejected += new MaskInputRejectedEventHandler(cpf_MaskInputRejected);
        }

